{
  "extends": "airbnb-base",
  "rules": {
    "no-param-reassign": 1,
    "import/extensions": 1,
    "import/no-unresolved": 1,
    "import/prefer-default-export": 1,
    "no-useless-escape": 1,
    "comma-dangle": 1,
    "no-unused-vars": 1,
    "func-names": 1,
    "one-var": 1,
    "one-var-declaration-per-line": 1,
    "no-lonely-if": 1,
    "no-shadow": 1,
    "no-console": 1,
    "import/first": 1,
    "no-return-assign": 1,
    "arrow-parens": 1,
    "prefer-template": 1,
    "padded-blocks": 1,
    "prefer-const": 1,
    "max-len": 1,
    "no-trailing-spaces": 1,
    "arrow-body-style": 1,
    "indent": 1,
    "space-in-parens": 1,
    "eol-last": 1,
    "consistent-return": 1,
    "global-require": 0,
    "no-underscore-dangle": 0
  }
}
